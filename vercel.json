{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/admin/refresh-cache",
            "schedule": "0 0 * * *"
        }
    ]
}